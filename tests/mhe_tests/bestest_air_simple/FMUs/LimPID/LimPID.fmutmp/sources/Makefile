PHONY: LimPID_FMU
LimPID_FMU: nozip
	cd .. && rm -f ../LimPID.fmu && zip -r ../LimPID.fmu *
nozip: LimPID_functions.h LimPID_literals.h $(OFILES) $(RUNTIMEFILES) $(FMISUNDIALSFILES)
	mkdir -p ../binaries/$(FMIPLATFORM)
ifeq (1,1)
	$(LD) -o LimPID$(DLLEXT) $(OFILES) $(RUNTIMEFILES) $(FMISUNDIALSFILES) -L"/home/marius/fmu_test/buildopt/test"   -Wl,-Bdynamic $(LDFLAGS)
	cp LimPID$(DLLEXT) LimPID_FMU.libs ../binaries/$(FMIPLATFORM)/
endif
	head -n20 Makefile > ../resources/$(FMIPLATFORM).summary
ifeq (@LIBTYPE_STATIC@,1)
	rm -f LimPID.a
	$(AR) -rsu LimPID.a $(OFILES) $(RUNTIMEFILES) $(FMISUNDIALSFILES)
	cp LimPID.a LimPID_FMU.libs ../binaries/$(FMIPLATFORM)/
endif
