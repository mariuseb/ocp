{
    "model": {
        "ode": {
            "Ti": "(Te - Ti)/(Rie*Ci) + (Ta - Ti)/(Ria*Ci) + Ai*(phi_s/Ci) + rho_int*((int_con+int_lat+int_rad)/Ci) + (1/Ci)*Prad_to_env",
            "Te": "(Ti - Te)/(Rie*Ce) + (Ta - Te)/(Rea*Ce)"
        },
        "w": {
            "v1": "Ti - y1"
        },
        "p": [
            "Rie",
            "Rea",
            "Ria",
            "Rirad",
            "Rrr",
            "Rsr",
            "rho_int",
            "Ci",
            "Ce",
            "Crad",
            "Cret",
            "Csup_nom",
            "Ai",
            "cp_water",
            "cp_air",
            "alpha_vent1",
            "alpha_vent2",
            "Tret_offset",
            "Prad_to_env_lim",
            "Trad_lim"
        ],
        "u": [
            "Prad_to_env"
        ],
        "r": [
            "Ta",
            "phi_s",
            "int_con",
            "int_lat",
            "int_rad"
        ],
        "x": [
            "Ti",
            "Te"
        ],
        "y": [
            ["y1", "Ti"]
        ]
    },
    "objective": "R[0,0]*dot(v1, v1) + Q[0,0]*dot(s1, s1) + Q[1,1]*dot(s2, s2)",
    "N": 96,
    "dt": 900,
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "legendre",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 10, 
        "ipopt.ma57_automatic_scaling": "yes", 
        "ipopt.max_cpu_time": 300,
        "ipopt.max_iter": 3000,
        "ipopt.check_derivatives_for_naninf": "yes",
        "ipopt.hessian_approximation": "exact"
    },
    "codegen": false
}
