{
    "model": {
        "ode": {
            "Ti": "(Te - Ti)/(Rie*Ci) + (Ta - Ti)/(Ria*Ci) + Ai*(phi_s/Ci) + rho_int*((int_con+int_lat+int_rad)/Ci) + (Trad - Ti)/(Rirad*Ci)",
            "Te": "(Ti - Te)/(Rie*Ce) + (Ta - Te)/(Rea*Ce)",
            "Trad": "rad_flo*cp_water*(dh_Tsup - Trad)/Crad + (Ti - Trad)/(Rirad*Crad)",
            "Tret": "(a2*rad_flo*(Trad - Tret))/(Rrr*Cret)"
        },
        "alg": {
            "Prad": "Prad - rad_flo*cp_water*eta_hex_rad*(dh_Tsup - Tret)",
            "rad_flo": "rad_flo - rad_flo_nom*regNonZeroPower(rad_val, n1, 1E-6)"
        },
        "w": {
            "v1": "y1 - Ti"
        },
        "p": [
            "cp_water",
            "eta_hex_ahu",
            "eta_hex_rad",
            "m_flow_air_nom",
            "m_flow_air_nom_ext",
            "cp_air",
            "eta_hex1",
            "Trec_off",
            "k",
            "ahu_pump_sup_thres",
            "n",
            "n1",
            "n2",
            "n3",
            "n4",
            "rad_flo_nom",
            "coi_flo_nom_b",
            "coi_flo_nom_a",
            "Tret_coi_b",
            "dh_Tsup",
            "a",
            "b",
            "c",
            "d",
            "Rie",
            "Rea",
            "Ria",
            "Rirad",
            "Rrr",
            "Rsr",
            "rho_int",
            "Ci",
            "Ce",
            "Crad",
            "Cret",
            "Csup_nom",
            "Ai",
            "alpha_vent1",
            "alpha_vent2",
            "Tret_offset",
            "c1",
            "CO2_out",
            "n_inf",
            "c_occ",
            "a1",
            "a2"
        ],
        "u": [
            "rad_val"
        ],
        "r": [
            "Ta",
            "phi_s",
            "int_con",
            "int_lat",
            "int_rad",
            "n_occ"
        ],
        "x": [
            "Ti",
            "Te",
            "Trad",
            "Tret"
        ],
        "z": [
            "Prad",
            "rad_flo"
        ],
        "y": [
            ["y1", "Ti"],
            ["y2", "Tret"],
            ["y3", "Prad"],
            ["y4", "rad_flo"]
        ]
    },
    "bounds": {
    "u": {
        "lbu": [0],
        "ubu": [1]
    },
    "z": {
        "lbz": [-1E3,0],
        "ubz": [1E7,10]
        }
    },
    "objective": "dot(v1, v1)@R[0,0] + Q[0,0]*dot(s1, s1) + Q[1,1]*dot(s2, s2) + Q[2,2]*dot(s3, s3) + Q[3,3]*dot(s4, s4) + Q[4,4]*dot(s5, s5) + Q[5,5]*dot(s6, s6)",
    "N": 96,
    "dt": 900,
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "legendre",
        "degree": 1,
        "n_steps": 1
        }
    ,
    "solver": "",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 10, 
        "ipopt.ma57_automatic_scaling": "yes", 
        "ipopt.max_cpu_time": 300,
        "ipopt.max_iter": 3000,
        "ipopt.check_derivatives_for_naninf": "yes",
        "ipopt.hessian_approximation": "exact"
    },
    "codegen": false
}
