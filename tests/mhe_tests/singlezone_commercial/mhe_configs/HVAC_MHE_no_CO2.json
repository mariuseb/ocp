{
    "model": {
        "ode": {
            "Trad": "rad_flo*cp_water*(dh_Tsup - Trad)/Crad + (Ti - Trad)/(Rirad*Crad)",
            "Tret": "(Trad - Tret)/(Rrr*Cret)*rad_val + (dh_Tsup - Tret_offset - Tret)/(Rsr*Cret)*(1-rad_val)"
        },
        "alg": {
            "Prad": "Prad - rad_flo*cp_water*eta_hex_rad*(dh_Tsup - Tret)",
            "rad_flo": "rad_flo - rad_flo_nom*regNonZeroPower(rad_val, n1, 1E-6)"
        },
        "p": [
            "cp_water",
            "eta_hex_ahu",
            "eta_hex_rad",
            "m_flow_air_nom",
            "m_flow_air_nom_ext",
            "cp_air",
            "eta_hex1",
            "Trec_off",
            "k",
            "ahu_pump_sup_thres",
            "n",
            "n1",
            "n2",
            "n3",
            "n4",
            "rad_flo_nom",
            "coi_flo_nom_b",
            "coi_flo_nom_a",
            "Tret_coi_b",
            "dh_Tsup",
            "a",
            "b",
            "c",
            "d",
            "Rie",
            "Rea",
            "Ria",
            "Rirad",
            "Rrr",
            "Rsr",
            "rho_int",
            "Ci",
            "Ce",
            "Crad",
            "Cret",
            "Csup_nom",
            "Ai",
            "alpha_vent1",
            "alpha_vent2",
            "Tret_offset",
            "c1",
            "CO2_out",
            "n_inf",
            "c_occ"
        ],
        "u": [
            "rad_val"
        ],
        "r": [
            "Ta",
            "phi_s",
            "int_con",
            "int_lat",
            "int_rad",
            "n_occ",
            "Prad_to_env_MPC",
            "Ti"
        ],
        "x": [
            "Trad",
            "Tret"
        ],
        "z": [
            "Prad",
            "rad_flo"
        ],
        "w": {
            "v1": "Tret - y2",
            "v2": "Prad - y3",
            "v3": "rad_flo - y4"
        },
        "y": [
            ["y2", "Tret"],
            ["y3", "Prad"],
            ["y4", "rad_flo"]
        ]
    },
    "objective": "R[0,0]*dot(v1, v1) + R[1,1]*dot(v2, v2) + R[2,2]*dot(v3, v3) + Q[0,0]*dot(s1, s1) + Q[1,1]*dot(s2, s2) + Q[2,2]*dot(s3, s3) + Q[3,3]*dot(s4, s4)",
    "N": 24,
    "dt": 900,
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "legendre",
        "degree": 1,
        "n_steps": 1
        }
    ,
    "solver": "",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 10, 
        "ipopt.ma57_automatic_scaling": "yes", 
        "ipopt.max_cpu_time": 15,
        "ipopt.max_iter": 3000,
        "ipopt.check_derivatives_for_naninf": "yes",
        "ipopt.hessian_approximation": "exact"
    },
    "codegen": false
}
