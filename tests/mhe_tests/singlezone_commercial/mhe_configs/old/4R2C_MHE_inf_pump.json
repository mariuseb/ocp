{
    "model": {
        "ode": {
            "Ti": "(Te - Ti)/(Rie*Ci) + (Ta - Ti)/(Ria*Ci) + (1/Ci)*dh_pump*UA_nom*regNonZeroPower((Tsup - Ti), n, 0.05) + Ai*(phi_s/Ci) + rho_int*((int_con+int_lat+int_rad)/Ci)",
            "Te": "(Ti - Te)/(Rie*Ce) + (Ta - Te)/(Rea*Ce)"
        },
        "w": {
            "v1": "Ti - y1", 
            "v2": "dh_pump*UA_nom*regNonZeroPower((Tsup - Ti), n, 0.05) - y2*eta"
        },
        "p": [
            "Rie",
            "Rea",
            "Ria",
            "rho_int",
            "Ci",
            "Ce",
            "Ai",
            "Tsup",
            "n",
            "UA_nom",
            "eta"
        ],
        "u": [
            "dh_pump",
            "Prad"
        ],
        "r": [
            "Ta",
            "phi_s",
            "int_con",
            "int_lat",
            "int_rad"
        ],
        "x": [
            "Ti",
            "Te"
        ],
        "y": [
            ["y1", "Ti"],
            ["y2", "Prad"]
        ]
    },
    "objective": "dot(v1, v1)@R[0,0] + dot(v2, v2)@R[1,1]",
    "N": 3,
    "dt": 900,
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "legendre",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 3,
        "ipopt.max_cpu_time": 100
    },
    "codegen": true,
    "with_jit": false
}