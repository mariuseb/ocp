{
    "model": {
        "ode": {
            "Ti": "(Te - Ti)/(Rie*Ci) + (kp*(Tset - Ti) + ki*E)/Ci + (Ai/Ci)*phi_s + s1",
            "Te": "(Ti - Te)/(Rie*Ce) + (Ta - Te)/(Rea*Ce) + s2",
            "E": "Tset - Ti + s3"

        },
        "w": {
            "v1": "y1 - Ti"
        },
        "p": [
            "Rie",
            "Rea",
            "Ci",
            "Ce",
            "Ai",
            "kp",
            "ki"
        ],
        "u": [
            "Tset"
        ],
        "r": [
            "Ta",
            "phi_s"
        ],
        "x": [
            "Ti",
            "Te",
            "E"
            ],
        "s": [
            "s1",
            "s2",
            "s3"
        ],
        "y": [
            ["y1", "Ti"]
        ]
    },
    "objective": "dot(v1, v1)@R[0,0] + dot(s1, s1)@Q[0,0] + dot(s2, s2)@Q[1,1] + dot(s3, s3)@Q[2,2]",
    "N": 12,
    "dt": 900,
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "legendre",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma27",
        "ipopt.ma57_pre_alloc": 3,
        "ipopt.max_cpu_time": 10
    },
    "codegen": false
}