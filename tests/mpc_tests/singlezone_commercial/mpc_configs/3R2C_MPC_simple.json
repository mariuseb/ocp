{
    "model": {
        "ode": {
            "Ti": "(Te - Ti)/(Rie*Ci) + (Ta - Ti)/(Ria*Ci) + Ai*(phi_s/Ci) + rho_int*((int_con+int_lat+int_rad)/Ci) + (1/Ci)*Pvent_to_env + (1/Ci)*Prad",
            "Te": "(Ti - Te)/(Rie*Ce) + (Ta - Te)/(Rea*Ce)",
            "CO2_in": "n_air*c1*(CO2_out - CO2_in) + c_occ*n_occ"
        },
        "alg": {
            "n_air": "n_air - (n_mech + n_nat + n_inf)",
            "n_mech": "n_mech - ahu_reaFloExtAir",
            "n_nat": "n_nat - (ahu_reaFloSupAir - ahu_reaFloExtAir)"
        },
        "p": [
            "Rie",
            "Rea",
            "Ria",
            "UA_vent_air",
            "rho_int",
            "Ci",
            "Ce",
            "Ai",
            "c1",
            "CO2_out",
            "n_inf",
            "c_occ"
        ],
        "u": [
            "Prad",
            "Pvent_to_env",
            "ahu_reaFloSupAir",
            "ahu_reaFloExtAir",
            "n_occ"
        ],
        "r": [
            "Ta",
            "phi_s",
            "int_con",
            "int_lat",
            "int_rad"
        ],
        "x": [
            "Ti",
            "Te",
            "CO2_in"
        ],
        "z": [
            "n_air",
            "n_mech",
            "n_nat"
        ]
    },
    "bounds": {
        "u": {
            "lbu": [0,0,0,0,288.15],
            "ubu": [1,1,1,1,313.15]
        },
        "x": {
            "lbx": [280,280,280,288.15,0],
            "ubx": [340,340,340,313.15,1500]
        },
        "z": {
            "lbz": [0,0,0,0,0,0,280,280,280,280,0,0,0,0,0,0,0,0],
            "ubz": [1E7,1E7,10,10,10,10,340,340,340,340,40,40,1,1E7,1E6,40,40,40]
        }
    },
    "objective": "(1/2)*dot(Pvent, Pvent) + (1/2)*dot(Prad, Prad) + (1/2)*dot(Pfan, Pfan)",
    "ocp": {
        "h": [
            "-0.05 <= ahu_pump_sup - ahu_pump_ret <= 0.05",
            "0 <= Tsup_air - Ti"
        ]
    },
    "solver": "",
    "N": 96,
    "dt": 900,
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "legendre",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 10, 
        "ipopt.ma57_automatic_scaling": "yes", 
        "ipopt.max_cpu_time": 300,
        "ipopt.max_iter": 3000,
        "ipopt.check_derivatives_for_naninf": "yes",
        "ipopt.hessian_approximation": "exact"
    },
    "codegen": false
}
