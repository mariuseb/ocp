{
    "model": {
        "ode": {
            "Ti": "(Te - Ti)/(Rie*Ci) + (Ta - Ti)/(Ria*Ci) + Ai*(phi_s/Ci) + rho_int*((int_con+int_lat+int_rad)/Ci) + (1/Ci)*ahu_reaFloSupAir*cp_air*(Tsup_air - Ti)",
            "Te": "(Ti - Te)/(Rie*Ce) + (Ta - Te)/(Rea*Ce)",
            "Tsup_air": "(oveTSupSet - Tsup_air)/tau_sup"
        },
        "alg": {
            "ahu_reaTRetAir": "ahu_reaTRetAir - Ti",
            "Pvent": "Pvent - m_flow_air_nom*ahu_pump_sup*cp_air*(Tsup_air-ahu_reaTHeaRec)*eta_hex1",
            "ahu_reaTHeaRec": "(Ta + (ahu_reaTRetAir - Ta)*eta_hex2)*ahu_pump_sup_on + (1-ahu_pump_sup_on)*Trec_off - ahu_reaTHeaRec",
            "ahu_reaFloSupAir": "ahu_reaFloSupAir - m_flow_air_nom*ahu_pump_sup",
            "ahu_pump_sup_on": "ahu_pump_sup_on - ca.if_else(ahu_pump_sup > 1E-3, 1, 0)",
            "ahu_pump_ret_on": "ahu_pump_ret_on - ca.if_else(ahu_pump_ret > 1E-3, 1, 0)"

        },
        "p": [
            "Rie",
            "Rea",
            "Ria",
            "rho_int",
            "Ci",
            "Ce",
            "Ai",
            "m_flow_air_nom",
            "Trec_off",
            "tau_sup",
            "cp_air",
            "eta_hex1",
            "eta_hex2"
        ],
        "u": [
            "ahu_pump_sup",
            "ahu_pump_ret",
            "oveTSupSet"
        ],
        "r": [
            "Ta",
            "phi_s",
            "int_con",
            "int_lat",
            "int_rad"
        ],
        "x": [
            "Ti",
            "Te",
            "Tsup_air"
        ],
        "z": [
            "ahu_reaTRetAir",
            "Pvent",
            "ahu_reaTHeaRec",
            "ahu_reaFloSupAir",
            "ahu_pump_sup_on",
            "ahu_pump_ret_on"
        ]
    },
    "bounds": {
        "u": {
            "lbu": [0,0,288.15],
            "ubu": [1,1,313.15]
        },
        "z": {
            "lbz": [280,0,280,0,0,0],
            "ubz": [320,1E7,320,40,1,1]
        }
    },
    "objective": "dot(Pvent, Pvent)",
    "ocp": {
        "h": [
            "-0.05 <= ahu_pump_sup - ahu_pump_ret <= 0.05",
            "0 <= Tsup_air - Ti"
        ]
    },
    "N": 6,
    "dt": 900,
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "legendre",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 10, 
        "ipopt.ma57_automatic_scaling": "yes", 
        "ipopt.max_cpu_time": 300,
        "ipopt.max_iter": 3000,
        "ipopt.check_derivatives_for_naninf": "yes",
        "ipopt.hessian_approximation": "exact"
    },
    "codegen": false
}
