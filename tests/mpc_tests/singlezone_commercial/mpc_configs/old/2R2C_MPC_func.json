{
    "model": {
        "ode": {
            "Ti": "(Te - Ti)/(Rie*Ci) + (1/Ci)*UA_nom*regNonZeroPower((dh_Tsup - Ti), n, 0.05)  + Ai*(phi_s/Ci)",
            "Te": "(Ti - Te)/(Rie*Ce) + (Ta - Te)/(Rea*Ce)"
        },
        "alg": {
            "Ph": "UA_nom*regNonZeroPower((dh_Tsup - Ti), n, 0.05) - Ph*eta"
        },
        "p": [
            "Rie",
            "Rea",
            "Ci",
            "Ce",
            "Ai",
            "n",
            "UA_nom",
            "eta"
        ],
        "u": [
            "dh_Tsup"
        ],
        "z": [
            "Ph"
        ],
        "r": [
            "Ta",
            "phi_s"
        ],
        "x": [
            "Ti",
            "Te"
        ]
    },
    "bounds": {
        "u": {
            "lbu": [283.15],
            "ubu": [333.15]
        }
    },
    "objective": "dot(Ph, Ph) + 1E10*dot(sl, sl)",
    "ocp": {
        "h": 
        [
            "0 <= dh_Tsup - Ti"        
        ]
    },
    "N": 12,
    "dt": 900,
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "legendre",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 3, 
        "ipopt.ma57_automatic_scaling": "yes", 
        "ipopt.max_cpu_time": 300,
        "ipopt.hessian_approximation": "exact"
    },
    "codegen": false
}