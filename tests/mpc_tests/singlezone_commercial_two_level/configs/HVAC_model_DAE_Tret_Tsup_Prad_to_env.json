            "Cret",
            "Csup",
            "Rsup",
            "Csup_off",
            "Rsup_off",
            "Csup_nom",
            "Riret",
            "Ai",
            "cp_water",
            "cp_air",
            "alpha_vent1",
            "alpha_vent2",
            "Tret_offset",
            "eta_hex_ahu",
            "eta_hex_rad",
            "m_flow_air_nom",
            "m_flow_air_nom_ext",
            "eta_hex1",
            "Trec_off",
            "k",
            "ahu_pump_sup_thres",
            "n",
            "n1",
            "n2",
            "n3",
            "n4",
            "rad_flo_nom",
            "coi_flo_nom_b",
            "coi_flo_nom_a",
            "Tret_coi_b",
            "dh_Tsup",
            "dh_Tsup_off",
            "a",
            "b",
            "c",
            "d",
            "a1",
            "a2"
        ],
        "u": [
            "rad_val",
            "coi_val",
            "ahu_pump_sup",
            "ahu_pump_ret",
            "Ti"
        ],
        "r": [
            "Ta",
            "phi_s",
            "int_con",
            "int_lat",
            "int_rad"
        ],
        "x": [
            "Trad",
            "Tret",
            "Tsup"
        ],
        "z": [
          "Prad",
          "rad_flo",
          "Prad_to_env"
        ],
        "y": [
            ["y1", "Ti"],
            ["y2", "Tret"],
            ["y3", "Tsup"],
            ["y4", "Prad"],
            ["y5", "Prad_to_env"],
            ["y6", "Trad"],
            ["y9", "rad_flo"]
        ]
    },
    "bounds": {
        "x": {
            "lbx": [280,280,280],
            "ubx": [340,340,340]
        },
        "z": {
            "lbz": [0,0,0],
            "ubz": [1E7,20,1E7]
        }
    },
    "objective": "dot(v1, v1)@R[0,0] + dot(v2, v2)@R[1,1] + dot(v3,v3)@R[2,2] + dot(v4, v4)@R[3,3] + dot(v5, v5)@R[4,4] + dot(v6, v6)@R[5,5] + dot(v9, v9)@R[6,6]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "radau",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "none",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 10,
        "ipopt.max_iter": 5000,
        "ipopt.ma57_automatic_scaling" : "yes"
    },
    "codegen": false
}
