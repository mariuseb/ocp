{
    "model": {
        "ode": {
            "Ti": "(Te - Ti)/(Rie*Ci) + (Ta - Ti)/(Ria*Ci) + Ai*(phi_s/Ci) + rho_int*((int_con+int_lat+int_rad)/Ci) + (1/Ci)*ahu_reaFloSupAir*cp_air*(oveTSupSet - Ti) + (1/Ci)*UA_nom*dh_pump*regNonZeroPower(dh_Tsup - Ti, n3, 0.05)",
            "Te": "(Ti - Te)/(Rie*Ce) + (Ta - Te)/(Rea*Ce)",
            "CO2_in": "n_air*c*(CO2_out - CO2_in) + c_occ*n_occ"
        },
        "alg": {
            "ahu_reaTRetAir": "(ahu_reaTRetAir - Ti)",
            "Pvent": "Pvent*eta_hex1 - (m_flow_air_nom_sup*regNonZeroPower(ahu_pump_sup, n, 0.01)*cp_air*(oveTSupSet-ahu_reaTHeaRec))",
            "ahu_reaTHeaRec": "(Ta + (ahu_reaTRetAir - Ta)*eta_hex2 - ahu_reaTHeaRec)",
            "ahu_reaFloSupAir": "ahu_reaFloSupAir - m_flow_air_nom_sup*regNonZeroPower(ahu_pump_sup, n, 0.01)",
            "ahu_reaFloExtAir": "ahu_reaFloExtAir - m_flow_air_nom_ret*regNonZeroPower(ahu_pump_ret, n2, 0.01)",
            "n_air": "n_air - (n_mech + n_nat + n_inf)",
            "n_mech": "n_mech - ahu_reaFloExtAir",
            "n_nat": "n_nat - m_flow_air_nom_sup*regNonZeroPower(ahu_pump_sup-ahu_pump_ret, n, 0.01)",
            "Prad": "Prad*eta_hex3 - UA_nom*dh_pump*regNonZeroPower(dh_Tsup - Ti, n3, 0.05)"
        },
        "p": [
            "Rie",
            "Rea",
            "Ria",
            "rho_int",
            "Ci",
            "Ce",
            "Ai",
            "m_flow_air_nom_sup",
            "m_flow_air_nom_ret",
            "dh_Tsup",
            "cp_air",
            "UA_nom",
            "eta_hex1",
            "eta_hex2",
            "eta_hex3",
            "n",
            "n2",
            "n3",
            "c",
            "CO2_out",
            "n_inf",
            "c_occ"
        ],
        "z": [
            "ahu_reaTRetAir",
            "Pvent",
            "ahu_reaTHeaRec",
            "ahu_reaFloSupAir", 
            "ahu_reaFloExtAir", 
            "n_air",
            "n_mech",
            "n_nat",
            "Prad"
        ],
        "r": [
            "Ta",
            "phi_s",
            "int_con",
            "int_lat",
            "int_rad",
            "n_occ"
        ],
        "u": [
             "ahu_pump_ret",
             "ahu_pump_sup",
             "oveTSupSet",
             "dh_pump"
        ],
        "x": [
            "Ti",
            "Te",
            "CO2_in"
        ]
    },
    "bounds": {
        "u": {
            "lbu": [0,0,288.15,0],
            "ubu": [1,1,313.15,1]
        },
        "z": {
            "lbz": [285.15,0,273.15,0,0,0,0,0,0],
            "ubz": [310,1E6,338.15,32,32,32,32,32,1E6]
        }
    },
    "objective": "dot(Pvent, Pvent) + dot(Prad, Prad)",
    "ocp": {
        "h": [
            "0 <= ahu_reaTRetAir - Ti",
            "-1E-5 <= ahu_pump_sup - ahu_pump_ret <= 1E-5"
        ]
    },
    "N": 12,
    "dt": 900,
    "method": "collocation",
    "integrator": 
        {
        "name": "IRK",
        "method": "legendre",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 3, 
        "ipopt.ma57_automatic_scaling": "yes", 
        "ipopt.max_cpu_time": 300,
        "ipopt.constr_viol_tol": 1E-4,
        "ipopt.check_derivatives_for_naninf": "yes",
        "ipopt.hessian_approximation": "exact"
    },
    "codegen": false
}
