{
    "model": {
        "ode": {
            "Ti": "(Ta - Ti)/(R*C) + phi_h/C"
        },
        "p": [
            "R",
            "C"
        ],
        "u": [
            "phi_h"
        ],
        "r": [
            "Ta"
        ],
        "x": [
            "Ti"
        ]
    },
    "bounds": {
        "u": {
            "lbu": [-3000],
            "ubu": [3000]
        }
    },
    "ocp": {
        "h": [
            "0 == Ti[0] - Ti[24]"
        ]
    },
    "objective": "dot(phi_h, phi_h)",
    "N": 25,
    "dt": 3600,
    "method": "collocation",
    "integrator": 
        {
        "name": "IRK",
        "method": "legendre",
        "degree": 1,
        "n_steps": 1
        }
    ,
    "solver": "none",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 3,
        "ipopt.ma57_automatic_scaling": "no",
        "ipopt.max_cpu_time": 1,
        "ipopt.hessian_approximation": "exact"
    },
    "codegen": false,
    "map_eval": true
}
