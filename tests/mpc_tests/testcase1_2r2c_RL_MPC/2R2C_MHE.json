{
    "model": {
        "ode": {
            "Ti": "(Te - Ti)/(Rie*Ci) + phi_h/Ci + (Ai/Ci)*phi_s",
            "Te": "(Ti - Te)/(Rie*Ce) + (Ta - Te)/(Rea*Ce)"

        },
        "w": {
            "v1": "y1 - Ti",
            "v2": "y2 - Te"
        },
        "p": [
            "Rie",
            "Rea",
            "Ci",
            "Ce",
            "Ai"        
        ],
        "u": [
            "phi_h"
        ],
        "r": [
            "Ta",
            "phi_s"
        ],
        "x": [
            "Ti",
            "Te"
            ],
        "y": [
            ["y1", "Ti"],
            ["y2", "Te"]
        ]
    },
    "objective": "dot(v1, v1)@R[0,0] + dot(v2, v2)@R[1,1]",
    "gamma": 1,
    "N": 100,
    "dt": 900,
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "radau",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "none",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 3,
        "ipopt.ma57_automatic_scaling": "yes",
        "ipopt.max_cpu_time": 100,
        "ipopt.print_level": 5,
        "ipopt.hessian_approximation": "exact"
    },
    "codegen": false
}
