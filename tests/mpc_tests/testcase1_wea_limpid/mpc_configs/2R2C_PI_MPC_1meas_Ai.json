{
    "model": {
        "ode": {
            "Ti": "(Te - Ti)/(Rie*Ci) + kp*((Tset - Ti) + E)/Ci + (Ai/Ci)*phi_s",
            "Te": "(Ti - Te)/(Rie*Ce) + (Ta - Te)/(Rea*Ce)",
            "E": "(Tset - Ti)*(kp/ki)"

        },
        "p": [
            "Rie",
            "Rea",
            "Ci",
            "Ce",
            "Ai",
            "kp",
            "ki"
        ],
        "u": [
            "Tset"
        ],
        "r": [
            "Ta",
            "phi_s"
        ],
        "x": [
            "Ti",
            "Te",
            "E"
            ]
    },
    "bounds": {
        "u": {
            "lbu": [288.15],
            "ubu": [313.15]
        }
    },
    "objective": "dot((kp*(Tset - Ti) + ki*E), (kp*(Tset - Ti) + ki*E))",
    "N": 12,
    "dt": 900,
    "method": "collocation",
    "integrator": 
        {
        "name": "IRK",
        "method": "radau",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 3,
        "ipopt.max_cpu_time": 2,
        "ipopt.ma57_automatic_scaling": "yes"
    },
    "codegen": false
}