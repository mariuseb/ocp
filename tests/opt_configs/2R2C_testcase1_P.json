{
    "model": {
        "ode": {
            "Ti": "(Te - Ti)/(Rie*Ci) + kp*(Tset - Ti)/Ci",
            "Te": "(Ti - Te)/(Rie*Ce) + (Ta - Te)/(Rea*Ce)"

        },
        "w": {
            "v1": "y1 - Ti"
        },
        "p": [
            "Rie",
            "Rea",
            "Ci",
            "Ce",
            "kp"
        ],
        "u": [
            "Tset"
        ],
        "x": [
            "Ti",
            "Te"
        ],
        "r": [
            "Ta",
            "phi_s"
        ],
        "y": [
            ["y1", "Ti"]
        ]
    },
    "objective": "dot(v1, v1)@R[0,0]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "radau",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 1,
        "ipopt.max_cpu_time": 200,
        "ipopt.ma57_automatic_scaling" : "yes"
    },
    "codegen": true
}