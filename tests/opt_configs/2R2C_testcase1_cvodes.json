{
    "model": {
        "ode": {
            "Ti": "(Te - Ti)/(Rie*Ci) + phi_h/Ci + (Ai/Ci)*phi_s",
            "Te": "(Ti - Te)/(Rie*Ce) + (Ta - Te)/(Rea*Ce)"

        },
        "w": {
            "v1": "y1 - Ti"
        },
        "p": [
            "Rie",
            "Rea",
            "Ci",
            "Ce",
            "Ai"
        ],
        "u": [
            "Ta",
            "phi_h",
            "phi_s"
        ],
        "x": [
            "Ti",
            "Te"
        ],
        "y": [
            ["y1", "Ti"]
        ]
    },
    "objective": "dot(v1, v1)@R[0,0]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "Cvodes"
        }
    ,
    "solver": "",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 10,
        "ipopt.ma57_automatic_scaling" : "yes"
    },
    "codegen": true,
    "map_eval": false
}