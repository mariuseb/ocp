PHONY: Pade_FMU
Pade_FMU: nozip
	cd .. && rm -f ../Pade.fmu && zip -r ../Pade.fmu *
nozip: Pade_functions.h Pade_literals.h $(OFILES) $(RUNTIMEFILES) $(FMISUNDIALSFILES)
	mkdir -p ../binaries/$(FMIPLATFORM)
ifeq (1,1)
	$(LD) -o Pade$(DLLEXT) $(OFILES) $(RUNTIMEFILES) $(FMISUNDIALSFILES) -L"/home/marius/Desktop/OM_wdir"   -Wl,-Bdynamic $(LDFLAGS)
	cp Pade$(DLLEXT) Pade_FMU.libs ../binaries/$(FMIPLATFORM)/
endif
	head -n20 Makefile > ../resources/$(FMIPLATFORM).summary
ifeq (@LIBTYPE_STATIC@,1)
	rm -f Pade.a
	$(AR) -rsu Pade.a $(OFILES) $(RUNTIMEFILES) $(FMISUNDIALSFILES)
	cp Pade.a Pade_FMU.libs ../binaries/$(FMIPLATFORM)/
endif
