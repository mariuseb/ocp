{
    "model": {
        "ode": {
            "Ti": "(Te - Ti)/(Rie*Ci) + (1/Ci)*phi_h + Ai*(phi_s/Ci) + s1",
            "Te": "(Ti - Te)/(Rie*Ce) + (Ta - Te)/(Rea*Ce) + s2"
        },
        "w": {
            "v1": "Ti - y1"
        },
        "p": [
            "Rie",
            "Rea",
            "Ci",
            "Ce",
            "Ai"
        ],
        "u": [
            "phi_h",
            "phi_s",
            "Ta"
        ],
        "x": [
            "Ti",
            "Te"
        ],
        "s": [
            "s1",
            "s2"
        ],
        "y": [
            ["y1", "Ti"]
        ]
    },
    "objective": "dot(v1, v1)@R[0,0] + dot(s1, s1)@Q[0,0] + (dot(s2, s2))@Q[1,1]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "RK4",
        "method": "legendre",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "mumps",
        "ipopt.ma57_pre_alloc": 10,
        "ipopt.ma57_automatic_scaling" : "yes"
    },
    "codegen": true
}
