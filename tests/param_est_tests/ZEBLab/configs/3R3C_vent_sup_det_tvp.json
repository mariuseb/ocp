{
    "model": {
        "ode": {
            "Ti": "(Te - Ti)/(Rie_TVP*Ci_TVP) + (Th - Ti)/(Rih_TVP*Ci_TVP) + Ai*(phi_s/Ci_TVP) + (1/Ci_TVP)*alpha_vent*V_sup_air*(T_sup_air - Ti) + (Ta - Ti)/(Ria*Ci_TVP)",
            "Te": "(Ti - Te)/(Rie_TVP*Ce_TVP) + (Ta - Te)/(Rea_TVP*Ce_TVP)", 
            "Th": "(Ti - Th)/(Rih_TVP*Ch_TVP) + phi_h*(1/Ch_TVP)"
        },
        "alg": {
            "Ci_TVP": "Ci_b + Ci_a*ca.if_else(V_sup_air > 0, 1, 0) - Ci_TVP",
            "Ce_TVP": "Ce_b + Ce_a*ca.if_else(V_sup_air > 0, 1, 0) - Ce_TVP",
            "Ch_TVP": "Ch_b + Ch_a*ca.if_else(V_sup_air > 0, 1, 0) - Ch_TVP",
            "Rie_TVP": "Rie_b + Rie_a*ca.if_else(V_sup_air > 0, 1, 0) - Rie_TVP",
            "Rea_TVP": "Rea_b + Rea_a*ca.if_else(V_sup_air > 0, 1, 0) - Rea_TVP",
            "Rih_TVP": "Rih_b + Rih_a*ca.if_else(V_sup_air > 0, 1, 0) - Rih_TVP"
        },
        "w": {
            "v1": "Ti - y1"
        },
        "p": [
            "Rie_b",
            "Rie_a",
            "Rea_b",
            "Rea_a",
            "Rih_b",
            "Rih_a",
            "Ria",
            "Ci_b",
            "Ci_a",
            "Ce_b",
            "Ce_a",
            "Ch_b",
            "Ch_a",
            "Ai",
            "alpha_vent"
        ],
        "u": [
            "phi_h",
            "phi_s", 
            "Ta",
            "V_sup_air",
            "T_sup_air"
        ],
        "x": [
            "Ti",
            "Te", 
            "Th"
        ],
        "z": [
            "Ce_TVP",
            "Ci_TVP",
            "Ch_TVP",
            "Rie_TVP",
            "Rea_TVP",
            "Rih_TVP"
        ],
        "y": [
             ["y1", "Ti"]
        ]
    },
    "bounds": {
        "z": {
            "lbz": [1E5,1E5,1E5,1E-5,1E-5,1E-5],
            "ubz": [1E9,1E9,1E9,1,1,1]
        }
    },
    "objective": "dot(v1, v1)@R",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "RK4",
        "method": "legendre",
        "degree": 3,
        "n_steps": 1
        }
    ,
