{
    "model": {
        "ode": {
            "Tsup": "(Tset_sup - Tsup)/(Rsup*Csup)*m_flow_bool + (Tset_sup - Tsup)/(Rsup_hold*Csup_hold)*(1-m_flow_bool)",
            "Th": "(1/tau_h)*((Tsup - Th_offset) - Th)",
            "Tret": "((Th - Tret_offset) - Tret)/(Rret*Cret)*m_flow_bool + ((Th - Tret_offset) - Tret)/(Rret_hold*Cret_hold)*(1 - m_flow_bool)"
        },
        "alg": {
            "Tset_sup": "Tset_sup - (T_set_sup_b - T_set_sup_a*(Ta - Ta_dim))*m_flow_bool - Tsup_ss*(1 - m_flow_bool)"
        },
        "w": {
            "v2": "Tret - y2",
            "v3": "Tsup - y3"
        },
        "p": [
            "tau_h",
            "Ta_dim",
            "T_set_sup_b",
            "T_set_sup_a",
            "Tsup_ss",
            "Tret_offset",
            "Th_offset",
            "x1_offset",
            "x2_offset",
            "x3_offset",
            "x4_offset",
            "Rsup",
            "Csup",
            "Rsup_hold",
            "Csup_hold",
            "Rret",
            "Cret",
            "Rret_hold",
            "Cret_hold",
            "d_close"
        ],
        "u": [
            "Ta",
            "Ti",
            "m_flow_bool",
            "m_flow_weight",
            "delta_m_flow",
            "u_val_set",
            "valve_open",
            "valve_weight",
            "rise"
        ],
        "z": [
            "Tset_sup"
        ],
        "x": [
          "Tsup",
          "Th",
          "Tret"
        ],
        "y": [
            ["y2", "Tret"],
            ["y3", "Tsup"]
        ]
    },
    "bounds": {
        "z": {
            "lbz": [280],
            "ubz": [320]
        }
    },
    "objective": "dot(m_flow_weight*v3, m_flow_weight*v3)@R[0,0] + dot(m_flow_weight*v2, m_flow_weight*v2)@R[1,1]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "legendre",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "",
    "opt": {
        "verbose": true,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 10,
        "ipopt.print_level": 5,
        "ipopt.ma57_automatic_scaling" : "yes",
        "ipopt.hessian_approximation": "exact"
    },
    "codegen": false,
    "map_eval": true
}
