{
    "model": {
        "ode": {
            "Tsup": "(Tset_sup - Tsup)/(Rsup*Csup)*m_flow_bool + (Tset_sup - Tsup)/(Rsup_hold*Csup_hold)*(1-m_flow_bool)",
            "x1": "(5/d_open)*((Tsup - x1_offset) - x1)*m_flow_bool + (5/d_close)*((Tsup - x1_offset) - x1)*(1-m_flow_bool)",
            "x2": "(5/d_open)*((x1 - x2_offset) - x2)*m_flow_bool + (5/d_close)*((x1 - x2_offset) - x2)*(1-m_flow_bool)",
            "Th": "(5/d_open)*((x2 - Th_offset) - Th)*m_flow_bool + (5/d_close)*((x2 - Th_offset) - Th)*(1-m_flow_bool)",
            "x3": "(5/d_open)*((Th - x3_offset) - x3)*m_flow_bool + (5/d_close)*((Th - x3_offset) - x3)*(1-m_flow_bool)",
            "x4": "(5/d_open)*((x3 - x4_offset) - x4)*m_flow_bool + (5/d_close)*((x3 - x4_offset) - x4)*(1-m_flow_bool)",
            "Tret": "((x4 - Tret_offset) - Tret)/(Rret*Cret)*m_flow_bool + ((x4 - Tret_offset) - Tret)/(Rret_hold*Cret_hold)*(1 - m_flow_bool)"
        },
        "alg": {
            "Tset_sup": "Tset_sup - (T_set_sup_b - T_set_sup_a*(Ta - Ta_dim))*m_flow_bool - Tsup_ss*(1 - m_flow_bool)"
        },
        "w": {
            "v2": "Tret - y2",
            "v3": "Tsup - y3"
        },
        "p": [
            "d_open",
            "Ta_dim",
            "T_set_sup_b",
            "T_set_sup_a",
            "Tsup_ss",
            "Tret_offset",
            "Th_offset",
            "x1_offset",
            "x2_offset",
            "x3_offset",
            "x4_offset",
            "Rsup",
            "Csup",
            "Rsup_hold",
            "Csup_hold",
            "Rret",
            "Cret",
            "Rret_hold",
            "Cret_hold",
            "d_close"
        ],
        "u": [
            "Ta",
            "Ti",
            "m_flow_bool",
            "m_flow_weight",
            "delta_m_flow",
            "u_val_set",
            "valve_open",
            "valve_weight",
            "rise"
        ],
        "z": [
            "Tset_sup"
        ],
        "x": [
          "Tsup",
          "x1",
          "x2",
          "Th",
          "x3",
          "x4",
          "Tret"
        ],
        "y": [
            ["y2", "Tret"],
            ["y3", "Tsup"]
        ]
    },
    "bounds": {
        "z": {
            "lbz": [280],
            "ubz": [320]
        }
    },
    "ocp": {
        "h": 
        [
            "0 <= Tret_offset - x2_offset",      
            "0 <= x2_offset - x1_offset",   
            "0 <= Th_offset - x2_offset",   
            "0 <= x3_offset - Th_offset",   
            "0 <= x4_offset - x3_offset"
        ]
    },
    "objective": "dot(valve_weight*v3, valve_weight*v3)@R[0,0] + dot(valve_weight*v2, valve_weight*v2)@R[1,1]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "legendre",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 10,
        "ipopt.ma57_automatic_scaling" : "yes",
        "ipopt.hessian_approximation": "exact"
    },
    "codegen": true
}
