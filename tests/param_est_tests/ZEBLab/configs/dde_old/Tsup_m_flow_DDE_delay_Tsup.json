{
    "model": {
        "ode": {
            "x1": "(3/d_sup)*(Tset_sup - x1)*m_flow_bool + (Tset_sup - x1)*(3/tau_off)*(1-m_flow_bool)",
            "x2": "(3/d_sup)*(x1 - x2)*m_flow_bool + (x1 - x2)*(3/tau_off)*(1-m_flow_bool)",
            "Tsup": "(x2 - Tsup)*(3/d_sup)*m_flow_bool + (x2 - Tsup)*(3/tau_off)*(1-m_flow_bool)",
            "x3": "(2/d)*(u_val_set - x3)",
            "x4": "(2/d)*(x3 - x4)"
        },
        "alg": {
            "Tset_sup": "Tset_sup - (T_set_sup_b - T_set_sup_a*(Ta - Ta_dim))*m_flow_bool - Tsup_ss*(1 - m_flow_bool)",
            "m_flow": "m_flow - m_flow_nom*x4"
        },
        "w": {
            "v3": "Tsup - y3",
            "v5": "m_flow - y5"
        },
        "p": [
            "d",
            "Ta_dim",
            "T_set_sup_b",
            "T_set_sup_a",
            "Tsup_ss",
            "tau_on",
            "tau_off",
            "m_flow_nom",
            "d_sup"
        ],
        "u": [
            "Ta",
            "Ti",
            "m_flow_bool",
            "flow_weight",
            "delta_m_flow",
            "u_val_set",
            "Tret",
            "valve_open"
        ],
        "z": [
            "Tset_sup",
            "m_flow"
        ],
        "x": [
            "x1",
            "x2",
            "Tsup",
            "x3",
            "x4"
        ],
        "y": [
            ["y3", "Tsup"],
            ["y5", "x2"]
        ]
    },
    "bounds": {
        "z": {
            "lbz": [280,-1],
            "ubz": [320,2]
        },
        "x": {
            "lbx": [280,280,280,-1,-1],
            "ubx": [320,320,320,2,2]
        }
    },
    "objective": "dot(flow_weight*v3, flow_weight*v3)@R[0,0] + dot(v5, v5)@R[1,1]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "legendre",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 100,
        "ipopt.ma57_print_level": 2,
        "ipopt.ma57_automatic_scaling" : "yes",
        "ipopt.hessian_approximation": "exact"
    },
    "codegen": true
}
