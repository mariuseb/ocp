{
    "model": {
        "ode": {
            "Ti": "(Te - Ti)/(Rie*Ci_TVP) + (Th - Ti)/(Rih*Ci_TVP) + Ai*(phi_s/Ci_TVP) + (1/Ci_TVP)*alpha_vent*V_sup_air*(T_sup_air - Ti) + (Ta - Ti)/(Ria*Ci_TVP)",
            "Te": "(Ti - Te)/(Rie*Ce_TVP) + (Ta - Te)/(Rea*Ce_TVP)",
            "Th": "(Ti - Th)/(Rih*Ch) + (1/Ch)*phi_h"
        },
        "alg": {
            "Ce_TVP": "Ce_TVP - (Ce + Ce_a*ca.if_else(V_flow > 0.01, 1, 0))",
            "Ci_TVP": "Ci_TVP - (Ci + Ci_a*ca.if_else(V_flow > 0.01, 1, 0))"
        },
        "w": {
            "v1": "Ti - y1"
        },
        "p": [
            "Rie",
            "Rea",
            "Rih",
            "Ria",
            "Ci",
            "Ci_a",
            "Ce",
            "Ce_a",
            "Ch",
            "Ai",
            "alpha_vent"
        ],
        "u": [
            "phi_h",
            "phi_s",
            "Ta",
            "V_sup_air",
            "T_sup_air",
            "V_flow"
        ],
        "z": [
            "Ce_TVP",
            "Ci_TVP"
        ],
        "x": [
            "Ti",
            "Te",
            "Th"
        ],
        "y": [
            ["y1", "Ti"]
        ]
    },
    "ocp": {
        "h": 
        [
            "0 <= Ce - Ci",
            "0 <= Ce - Ch",
            "0 <= phi_h*(Th - Ti)"
        ]
    },
    "bounds": {
        "z": {
            "lbz": [1E5,1E5],
            "ubz": [1E8,1E8]
        }
    },
    "objective": "dot(v1, v1)@R[0,0]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "legendre",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 10,
        "ipopt.ma57_automatic_scaling" : "yes"
    },
    "codegen": true
}