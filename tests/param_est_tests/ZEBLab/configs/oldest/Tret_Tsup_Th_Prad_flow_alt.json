{
    "model": {
        "ode": {
            "Tret": "(Th - Tret)/(Rrh*Cret)*m_flow_bool + (Ti - Tret)/(Rret_hold*Cret_hold)*(1-m_flow_bool)",
            "Tsup": "((Tset_sup - Tsup)/(Rsup*Csup) + (Th - Tsup)/(Rsh*Csup))*m_flow_bool + ((Tset_sup - Tsup)/(Rsup_hold*Csup_hold))*(1-m_flow_bool)",
            "Th": "Prad/Ch + ((Ti - deltaT_h) - Th)/(Rih*Ch)"
        },
        "alg": {
            "Tset_sup": "Tset_sup - (T_set_sup_b - T_set_sup_a*(Ta - Ta_dim))*m_flow_bool - (Ti + deltaT_sup)*(1 - m_flow_bool)"
        },
        "w": {
            "v1": "m_flow_weight*(Tret - y1)",
            "v2": "m_flow_weight*(Tsup - y2)"
        },
        "p": [
            "Ta_dim",
            "T_set_sup_b",
            "T_set_sup_a",
            "Ta_max",
            "Rsup",
            "Csup",
            "Rih",
            "Rrh",
            "Rsh",
            "Rir",
            "Ch",
            "Ch_hold",
            "Rret_hold",
            "Cret_hold",
            "Rsup_hold",
            "Csup_hold",
            "Rret",
            "Cret",
            "deltaT_h1",
            "deltaT_h2",
            "deltaT_h",
            "deltaT_sup",
            "deltaT_ret",
            "tau",
            "m_flow_nom",
            "cp_water"
        ],
        "u": [
            "Ta",
            "Ti",
            "m_flow_bool",
            "m_flow_weight",
            "delta_m_flow",
            "u_val_set",
            "Tret_rise",
            "rise",
            "m_flow_ss",
            "fall",
            "Prad"
        ],
        "z": [
            "Tset_sup"
        ],
        "x": [
          "Tret",
          "Tsup",
          "Th"
        ],
        "y": [
            ["y1", "Tret"],
            ["y2", "Tsup"]
        ]
    },
    "bounds": {
        "z": {
            "lbz": [280],
            "ubz": [320]
        }
    },
    "objective": "dot(v1, v1)@R[0,0] + dot(v2, v2)@R[1,1]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "legendre",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 10,
        "ipopt.ma57_automatic_scaling" : "yes",
        "ipopt.hessian_approximation": "exact"
    },
    "codegen": true
}