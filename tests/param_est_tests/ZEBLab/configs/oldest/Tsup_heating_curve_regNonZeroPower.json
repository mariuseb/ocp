{
    "model": {
        "alg": {
            "Tsup": "Tsup - (T_set_sup_b - T_set_sup_a*(Ta - Ta_dim))",
            "Prad":  "Prad - m_flow*UA_nom*regNonZeroPower((Tsup - Ti), n, delta)"
        },
        "w": {
            "v1": "Tsup - y1",
            "v2": "Prad - y2"
        },
        "p": [
            "Ta_dim",
            "T_set_sup_b",
            "T_set_sup_a",
            "Ta_max",
            "UA_nom",
            "n",
            "delta"
        ],
        "u": [
            "u_val",
            "Ta",
            "Ti",
            "m_flow_bool",
            "m_flow"
        ],
        "z": [
          "Tsup",
          "Prad"
        ],
        "y": [
            ["y1", "Tsup"],
            ["y2", "Prad"]
        ]
    },
    "objective": "dot(m_flow_bool*v1, m_flow_bool*v1)@R[0,0] + dot(v2, v2)@R[1,1]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "legendre",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 10,
        "ipopt.ma57_automatic_scaling" : "yes"
    },
    "codegen": true
}