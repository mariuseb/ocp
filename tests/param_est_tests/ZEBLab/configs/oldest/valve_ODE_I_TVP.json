{
    "model": {
        "ode": {
            "u_val": "(u_val_set - u_val)*(1/tau_fall)",
            "x_tau_fall": "(u_val - x_tau_fall)*(1/tau_delta_T)"

        },
        "alg": {
            "m_flow": "m_flow - m_flow_nom*u_val",
            "tau_fall": "tau_fall - (tau_fall_b + tau_fall_a*x_tau_fall)"
        },
        "w": {
            "v1": "m_flow - y1"
        },
        "p": [
            "tau",
            "tau_fall_a",
            "tau_fall_b",
            "tau_delta_T",
            "m_flow_nom"
        ],
        "u": [
            "u_val_set",
            "m_flow_bool",
            "u_val_set_fall",
            "deltaT",
            "rise"
        ],
        "z": [
          "m_flow",
          "tau_fall"
        ],
        "x": [
          "u_val",
          "x_tau_fall"
        ],
        "y": [
            ["y1", "m_flow"]
        ]
    },
    "bounds": {
        "z": {
            "lbz": [0,1],
            "ubz": [0.01,10000]
        }
    },
    "objective": "dot(v1, v1)@R[0,0]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "legendre",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma86",
        "ipopt.ma57_pre_alloc": 10,
        "ipopt.ma57_automatic_scaling" : "yes"
    },
    "codegen": true
}