{
    "model": {
        "alg": {
            "m_flow": "m_flow - m_flow_nom*equalPercentage(u_val,R,l,delta)"
        },
        "w": {
            "v1": "m_flow - y1"
        },
        "p": [
            "R",
            "l",
            "delta",
            "m_flow_nom"
        ],
        "u": [
            "u_val"
        ],
        "z": [
          "m_flow"
        ],
        "y": [
            ["y1", "m_flow"]
        ]
    },
    "objective": "dot(v1, v1)@R[0,0]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "legendre",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 10,
        "ipopt.ma57_automatic_scaling" : "yes"
    },
    "codegen": true
}