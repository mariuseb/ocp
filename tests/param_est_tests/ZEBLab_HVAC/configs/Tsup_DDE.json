{
    "model": {
        "ode": {
            "x1": "(3/d_sup)*(Tset_sup - x1)",
            "x2": "(3/d_sup)*(x1 - x2)",
            "Tsup": "(x2 - Tsup)*(3/d_sup)",
            "flow": "((u_val_set - flow)/tau)"
        },
        "alg": {
            "Tset_sup": "Tset_sup - (T_set_sup_b - T_set_sup_a*(Ta - Ta_dim))*m_flow_bool - Tsup_ss*(1 - m_flow_bool)"
        },
        "w": {
            "v3": "Tsup - y3"
        },
        "p": [
            "d",
            "Ta_dim",
            "T_set_sup_b",
            "T_set_sup_a",
            "Tsup_ss",
            "tau",
            "tau_off",
            "m_flow_nom",
            "d_sup"
        ],
        "u": [
            "Ta",
            "Ti",
            "m_flow_bool",
            "flow_weight",
            "delta_m_flow",
            "u_val_set",
            "Tret",
            "valve_open",
            "m_flow"
        ],
        "z": [
            "Tset_sup"
        ],
        "x": [
            "x1",
            "x2",
            "Tsup",
            "flow"
        ],
        "y": [
            ["y3", "Tsup"]
        ]
    },
    "bounds": {
        "z": {
            "lbz": [280],
            "ubz": [320]
        },
        "x": {
            "lbx": [280,280,280,-2],
            "ubx": [320,320,320,2]
        }
    },
    "objective": "dot(flow_weight*v3, flow_weight*v3)@R[0,0]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "legendre",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 100,
        "ipopt.ma57_print_level": 2,
        "ipopt.ma57_automatic_scaling" : "yes",
        "ipopt.hessian_approximation": "exact"
    },
    "codegen": false
}
