{
    "model": {
        "ode": {
            "Tsup": "(Tset_sup - Tsup)/(Rsup*Csup)*valve_open + (Tset_sup - Tsup)/(Rsup_hold*Csup_hold)*(1-valve_open)",
            "x1": "(2/d)*((Tsup - x1_offset) - x1)",
            "x2": "(2/d)*((x1 - x2_offset) - x2)",
            "Tret": "((x2 - Tret_offset) - Tret)/(Rret*Cret)*valve_open + ((x2 - Tret_offset) - Tret)/(Rret_hold*Cret_hold)*(1 - valve_open)"
        },
        "alg": {
            "Tset_sup": "Tset_sup - (T_set_sup_b - T_set_sup_a*(Ta - Ta_dim))*valve_open - Tsup_ss*(1 - valve_open)"
        },
        "w": {
            "v2": "Tret - y2",
            "v3": "Tsup - y3"
        },
        "p": [
            "d",
            "Ta_dim",
            "T_set_sup_b",
            "T_set_sup_a",
            "Tsup_ss",
            "Tret_offset",
            "x1_offset",
            "x2_offset",
            "Rsup",
            "Csup",
            "Rsup_hold",
            "Csup_hold",
            "Rret",
            "Cret",
            "Rret_hold",
            "Cret_hold"
        ],
        "u": [
            "Ta",
            "Ti",
            "m_flow_bool",
            "m_flow_weight",
            "delta_m_flow",
            "u_val_set",
            "valve_open",
            "valve_weight"
        ],
        "z": [
            "Tset_sup"
        ],
        "x": [
          "Tsup",
          "x1",
          "x2",
          "Tret"
        ],
        "y": [
            ["y2", "Tret"],
            ["y3", "Tsup"]
        ]
    },
    "bounds": {
        "z": {
            "lbz": [280],
            "ubz": [320]
        }
    },
    "ocp": {
        "h": 
        [
            "0 <= Tret_offset - x2_offset",      
            "0 <= x2_offset - x1_offset"      
        ]
    },
    "objective": "dot(valve_weight*v3, valve_weight*v3)@R[0,0] + dot(valve_weight*v2, valve_weight*v2)@R[1,1]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "legendre",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 10,
        "ipopt.ma57_automatic_scaling" : "yes",
        "ipopt.hessian_approximation": "exact"
    },
    "codegen": true
}
