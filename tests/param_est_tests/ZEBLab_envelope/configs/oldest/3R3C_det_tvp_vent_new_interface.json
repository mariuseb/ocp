{
    "model": {
        "ode": {
            "Ti": "(Te - Ti)/(Rie*Ci) + (Th - Ti)/(Rih*Ci) + Ai*(phi_s/Ci) + (1/Ci)*alpha_vent*V_sup_air*(T_sup_air - Ti) + (Ta - Ti)/(Ria*Ci)",
            "Te": "(Ti - Te)/(Rie*Ce) + (Ta - Te)/(Rea*Ce)",
            "Th": "(Ti - Th)/(Rih*Ch) + (1/Ch)*phi_h"
        },
        "w": {
            "v1": "Ti - y1",
            "Ci": "Ci_b + Ce_a*Ti",
            "Ce": "Ce_b + Ce_a*Te",
            "Rie": "Rie_b + Rie_a*ca.if_else(V_sup_air > 10, 1, 0)"
        },
        "p": [
            "Rie_b",
            "Rie_a",
            "Rea",
            "Rih",
            "Ria",
            "Ci_b",
            "Ci_a",
            "Ce_b",
            "Ce_a",
            "Ch",
            "Ai",
            "alpha_vent"
        ],
        "u": [
            "phi_h",
            "phi_s",
            "Ta",
            "V_sup_air",
            "T_sup_air"
        ],
        "x": [
            "Ti",
            "Te",
            "Th"
        ],
        "y": [
            ["y1", "Ti"]
        ]
    },
    "bounds": {
        "w": {
            "lbw": [1E5,1E6,1E-5],
            "ubw": [1E9,1E10,1]
        }
    },
    "objective": "dot(v1, v1)@R[0,0]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "legendre",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 10,
        "ipopt.ma57_automatic_scaling" : "yes"
    },
    "codegen": true
}