{
    "model": {
        "alg": {
            "Tret": "Tret - (T_set_ret_b - T_set_ret_a*(Ta - Ta_dim))",
            "Tsup": "Tsup - (T_set_sup_b - T_set_sup_a*(Ta - Ta_dim))"
        },
        "w": {
            "v1": "Tret - y1",
            "v2": "Tsup - y2"
        },
        "p": [
            "Ta_dim",
            "T_set_ret_b",
            "T_set_ret_a",
            "T_set_sup_b",
            "T_set_sup_a"
        ],
        "u": [
            "u_val",
            "Ta",
            "Ti",
            "m_flow_bool"
        ],
        "z": [
          "Tret",
          "Tsup"
        ],
        "y": [
            ["y1", "Tret"],
            ["y2", "Tsup"]
        ]
    },
    "objective": "dot(m_flow_bool*v1, m_flow_bool*v1)@R[0,0] + dot(m_flow_bool*v2, m_flow_bool*v2)@R[1,1]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "legendre",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 10,
        "ipopt.ma57_automatic_scaling" : "yes"
    },
    "codegen": true
}