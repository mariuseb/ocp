{
    "model": {
        "ode": {
            "u_val": "(u_val_set - u_val)*(1/tau)",
            "e": "u_val*(1/tau_e)"

        },
        "alg": {
            "m_flow": "m_flow - m_flow_nom*(alpha1*u_val + alpha2*e)"
        },
        "w": {
            "v1": "m_flow - y1"
        },
        "p": [
            "tau",
            "tau_e",
            "m_flow_nom",
            "alpha1",
            "alpha2"
        ],
        "u": [
            "u_val_set",
            "m_flow_bool",
            "u_val_set_fall"
        ],
        "z": [
          "m_flow"
        ],
        "x": [
          "u_val",
          "e"
        ],
        "y": [
            ["y1", "m_flow"]
        ]
    },
    "objective": "dot(v1, v1)@R[0,0]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "legendre",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma86",
        "ipopt.ma57_pre_alloc": 10,
        "ipopt.ma57_automatic_scaling" : "yes"
    },
    "codegen": true
}