{
    "model": {
        "ode": {
            "Ti": "(Te - Ti)/(Rie*Ci) + _y*UA_is*(_y*(313.15 - 294.15) + 294.15 - Ti)/Ci + Ai*(phi_s/Ci)",
            "Te": "(Ti - Te)/(Rie*Ce) + (Ta - Te)/(Rea*Ce)",
            "E": "pi_ode(E, ca.vertcat(Ti, Tset))"
        },
        "w": {
            "v1": "Ti - y1"
        },
        "alg": {
            "_y": "_y + pi_y(E, ca.vertcat(Ti, Tset))"
        },
        "p": [
            "Rie",
            "Rea",
            "Ci",
            "Ce",
            "Ai",
            "UA_is"
        ],
        "u": [
            "Tset"
        ],
        "z": [
            "_y"
        ],
        "r": [
            "Ta",
            "phi_s",
            "Ti_ub",
            "Ti_lb"
        ],
        "x": [
            "Ti",
            "Te",
            "E"
        ],
    "y": [
        ["y1", "Ti"]
    ]
    },
    "objective": "dot(v1, v1)@R[0,0]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "radau",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "none",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 10,
        "ipopt.ma57_automatic_scaling" : "yes"
    },
    "codegen": false,
    "map_eval": false
}