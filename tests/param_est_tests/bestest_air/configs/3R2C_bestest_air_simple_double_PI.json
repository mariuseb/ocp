            "E": "Tset - Ti"
        },
        "alg":
            {
            "Tsup": "(kp*(Tset - Ti) + ki*E)*(313.15 - 294.15) + 294.15",
            "u_fan": "(kp*(Tset - Ti) + ki*E)"
        },
        "z": [
            "Tsup",
            "u_fan" 
        ],
        "p": [
            "Rie",
            "Rea",
            "Ci",
            "Ce",
            "Ai",
            "UA_is",
            "kp",
            "ki"
        ],
        "u": [
            "Tset"
        ],
        "r": [
            "Ta",
            "phi_s",
            "Ti_ub",
            "Ti_lb"
        ],
        "x": [
            "Ti",
            "Te",
            "E"
        ],
    "y": [
        ["y1", "Ti"]
    ]
    },
    "objective": "dot(v1, v1)@R[0,0]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "idas"
        }
    ,
    "solver": "none",
    "opt": {
