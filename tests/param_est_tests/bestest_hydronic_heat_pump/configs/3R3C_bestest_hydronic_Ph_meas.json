{
    "model": {
        "ode": {
            "dTi": "(Te - Ti)/(Rie*Ci) + Ph/Ch + Ai*(phi_s/Ci) + w1",
            "dTe": "(Ti - Te)/(Rie*Ce) + (Ta - Te)/(Rea*Ce) + w2"
        },
        "alg": {
            "h1": "y1 - Ti - v1",
            "h2": "y2 - Ph - v2",
            "g1": "Ph - (1 - ca.exp((-UA_nom*(Tsup - Ph/(2*Ch) - Ti)/dT_nom)**1.24)/Ch)*Ch*(Tsup - Ti)"
        },
        "p": [
            "Rie",
            "Rea",
            "Ci",
            "Ce",
            "Ch",
            "Ai",
            "UA_nom",
            "dT_nom"
        ],
        "u": [
            "Tsup",
            "Ta", 
            "phi_s"
        ],
        "x": [
            "Ti",
            "Te"
        ],
        "z": [
            "Ph"
        ],
        "w": [
            "w1",
            "w2"
        ],
        "v": [
            "v1",
            "v2"
        ],
        "y": [
            ["y1", "Ti"],
            ["y2", "Ph"]
        ]
    },
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "legendre",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "gauss-newton",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 3
    },
    "codegen": false
}