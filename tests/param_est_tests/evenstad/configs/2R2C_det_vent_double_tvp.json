        "alg": {
            "Ci_TVP": "Ci + Ci_a1*vent_on + Ci_a2*weeknd - Ci_TVP",
            "Ce_TVP": "Ce + Ce_a1*vent_on + Ce_a2*weeknd  - Ce_TVP",
            "Rie_TVP": "Rie + Rie_a1*vent_on + Rie_a2*weeknd - Rie_TVP",
            "Rea_TVP": "Rea + Rea_a1*vent_on + Rea_a2*weeknd - Rea_TVP"
        },
        "w": {
            "v1": "Ti - y1"
        },
        "p": [
            "Rie",
            "Rie_a1",
            "Rie_a2",
            "Rea",
            "Rea_a1",
            "Rea_a2",
            "Ci",
            "Ci_a1",
            "Ci_a2",
            "Ce",
            "Ce_a1",
            "Ce_a2",
            "Ai",
            "Ae",
            "alpha_vent"
        ],
        "u": [
            "phi_h",
            "phi_s",
            "Ta",
            "V_flow_sup_air",
            "Tsup_air",
            "weeknd",
            "vent_on"
        ],
        "z": [
            "Ce_TVP",
            "Ci_TVP",
            "Rie_TVP",
            "Rea_TVP"
        ],
        "x": [
            "Ti",
            "Te"
        ],
        "y": [
            ["y1", "Ti"]
        ]
    },
    "bounds": {
        "z": {
            "lbz": [1E6,1E6,1E-5,1E-5],
            "ubz": [1E10,1E10,100,100]
        }
    },
    "objective": "dot(v1, v1)@R[0,0]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "legendre",
        "degree": 3,
        "n_steps": 1
        }
    ,
