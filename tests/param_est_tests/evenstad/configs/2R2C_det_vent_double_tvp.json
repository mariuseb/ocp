{
    "model": {
        "ode": {
            "Ti": "(Te - Ti)/(Rie_TVP*Ci_TVP) + (1/Ci_TVP)*phi_h + Ai*(phi_s/Ci_TVP) + (1/Ci_TVP)*alpha_vent*V_flow_sup_air*(Tsup_air - Ti)",
