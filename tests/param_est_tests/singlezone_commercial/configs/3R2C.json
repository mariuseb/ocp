{
    "model": {
        "ode": {
            "Ti": "(Te - Ti)/(Rie*Ci) + (Ta - Ti)/(Ria*Ci) + Ai*(phi_s/Ci) + rho_int*((int_con+int_lat+int_rad)/Ci) + (1/Ci)*UA_nom_air*(Tsup_air - Ti)*ahu_pump_sup + (1/Ci)*Prad",
            "Te": "(Ti - Te)/(Rie*Ce) + (Ta - Te)/(Rea*Ce)"
        },
        "w": {
            "v1": "Ti - y1"
        },
        "p": [
            "Rie",
            "Rea",
            "Ria",
            "UA_nom_air",
            "rho_int",
            "Ci",
            "Ce",
            "Ai"
        ],
        "u": [
            "Pvent",
            "Prad",
            "Tsup_air",
            "ahu_pump_sup"
        ],
        "r": [
            "Ta",
            "phi_s",
            "int_con",
            "int_lat",
            "int_rad"
        ],
        "x": [
            "Ti",
            "Te"
        ],
        "y": [
            ["y1", "Ti"]
        ]
    },
    "objective": "dot(v1, v1)@R[0,0]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "radau",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "none",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 10,
        "ipopt.ma57_automatic_scaling" : "yes"
    },
    "codegen": false
}
