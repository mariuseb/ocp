            "Ria",
            "rho_int",
            "Ci",
            "Ce",
            "Ai",
            "m_flow_air_nom",
            "cp_air",
            "eta_hex1",
            "eta_hex2"
        ],
        "u": [
            "Pvent",
            "ahu_pump_sup",
            "ahu_pump_ret",
            "ahu_reaTHeaRec",
            "ahu_reaFloSupAir",
            "ahu_reaTSupAir"
        ],
        "r": [
            "Ta",
            "phi_s",
            "int_con",
            "int_lat",
            "int_rad"
        ],
        "x": [
            "Ti",
            "Te"
        ],
        "z": [
            "ahu_reaTRetAir"
        ],
        "y": [
            ["y1", "Ti"],
            ["y2", "Pvent"],
            ["y3", "ahu_reaTHeaRec"],
            ["y4", "ahu_reaFloSupAir"],
            ["y5", "ahu_reaTRetAir"]
        ]
    },
    "objective": "dot(v1, v1)@R[0,0]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "radau",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "none",
    "opt": {
        "verbose": false,
