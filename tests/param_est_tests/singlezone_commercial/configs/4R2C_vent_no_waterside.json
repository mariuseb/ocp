            "v3": "ahu_pump_ret*(Ta + (ahu_reaTRetAir - Ta)*eta_hex2 - ahu_reaTHeaRec)",
            "v4": "ahu_reaFloSupAir - m_flow_air_nom*ahu_pump_sup",
            "v5": "ahu_pump_ret*(y5 - ahu_reaTRetAir)"
        },
        "p": [
            "Rie",
