{
    "model": {
        "ode": {
            "Ti": "(Te - Ti)/(Rie*Ci) + (Ta - Ti)/(Ria*Ci) + Ai*(phi_s/Ci) + rho_int*((int_con+int_lat+int_rad)/Ci) + (1/Ci)*alpha_vent1*cp_air*ahu_reaFloSupAir*(Tsup_air - Ti) + (Trad - Ti)/(Rirad*Ci)",
            "Te": "(Ti - Te)/(Rie*Ce) + (Ta - Te)/(Rea*Ce)",
            "Trad": "rad_flo*cp_water*(Tsup - Trad)/Crad + (Ti - Trad)/(Rirad*Crad)",
            "Tret": "(a2*rad_flo*(Trad - Tret))/(Rrr*Cret)",
            "Tsup_air": "ahu_reaFloSupAir*cp_air*(ahu_reaTHeaRec - Tsup_air)/(Csup_nom) + ahu_reaFloSupAir*alpha_vent2*cp_air*(Ti - Tsup_air)/(Csup_nom)  + (1/Csup_nom)*Pvent"
        },
        "w": {
            "v1": "Ti - y1",
            "v2": "Tret - y2",
            "v3": "Tsup_air - y3"
        },
        "p": [
            "Rie",
            "Rea",
            "Ria",
            "Rirad",
            "Rrr",
            "Rsr",
            "rho_int",
            "Ci",
            "Ce",
            "Crad",
            "Cret",
            "Csup_nom",
            "Ai",
            "cp_water",
            "cp_air",
            "alpha_vent1",
            "alpha_vent2",
            "Tret_offset",
            "a2"
        ],
        "u": [
            "Pvent",
            "Prad",
            "ahu_pump_sup",
            "rad_flo",
            "ahu_reaFloSupAir",
            "Tsup",
            "rad_flo_on",
            "ahu_reaTHeaRec",
            "rad_val"
        ],
        "r": [
            "Ta",
            "phi_s",
            "int_con",
            "int_lat",
            "int_rad"
        ],
        "x": [
            "Ti",
            "Te",
            "Trad",
            "Tret",
            "Tsup_air"
        ],
        "y": [
            ["y1", "Ti"],
            ["y2", "Tret"],
            ["y3", "Tsup_air"]
        ]
    },
    "bounds": {
        "x": {
            "lbx": [280,280,280,280,280],
            "ubx": [340,340,340,340,340]
        }
    },
    "objective": "dot(v1, v1)@R[0,0] + dot(v2, v2)@R[1,1] + dot(v3, v3)@R[2,2]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "radau",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "none",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 10,
        "ipopt.ma57_automatic_scaling" : "yes"
    },
    "codegen": false
}
