            "rad_flo_on",
            "Ti"
        ],
        "r": [
            "Ta",
            "phi_s",
            "int_con",
            "int_lat",
            "int_rad"
        ],
        "x": [
            "Trad",
            "Tret"
        ],
        "z": [
          "Prad",
          "rad_flo"
        ],
        "y": [
            ["y1", "Ti"],
            ["y2", "Tret"],
            ["y4", "Prad"],
            ["y9", "rad_flo"]
        ]
    },
    "bounds": {
        "x": {
            "lbx": [280,280],
            "ubx": [340,340]
        },
        "z": {
            "lbz": [0,0,0,0,0,0,280,280,280,0,0,0,0],
            "ubz": [1E7,1E7,10,10,10,10,340,340,340,40,40,1,1E6]
        }
    },
    "objective": "dot(v1, v1)@R[0,0] + dot(rad_flo_on*v2, rad_flo_on*v2)@R[1,1] + dot(v4, v4)@R[2,2] + dot(v9, v9)@R[3,3]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "radau",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "none",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 10,
        "ipopt.ma57_automatic_scaling" : "yes"
    },
    "codegen": false
}
