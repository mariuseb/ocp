{
    "model": {
        "ode": {
            "Trad": "rad_flo*cp_water*(dh_Tsup - Trad)/Crad + (Ti - Trad)/(Rirad*Crad)",
            "Tret": "(Trad - Tret)/(Rrr*Cret)*rad_val"
        },
        "alg": {
            "Prad": "Prad - rad_flo*cp_water*eta_hex_rad*(dh_Tsup - Tret)",
            "rad_flo": "rad_flo - rad_flo_nom*regNonZeroPower(rad_val, n1, 1E-6)"
        },
        "w": {
            "v1": "Ti - y1",
            "v2": "Tret - y2",
            "v4": "y4 - Prad",
            "v9": "y9 - rad_flo"
        },
        "p": [
