{
    "model": {
        "ode": {
            "Tret": "(Th - Tret)/(Rrh*Cret) + (Ti + deltaT_ret - Tret)/(Rret*Cret)",
            "Tsup": "((Tset_sup - Tsup)/(Rsup*Csup) + (Th - Tsup)/(Rsh*Csup))*m_flow_bool + ((Tset_sup - Tsup)/(Rsup_hold*Csup_hold) + (Th - Tsup)/(Rsh*Csup_hold))*(1-m_flow_bool)",
            "Th": "Prad/Ch + ((Ti - deltaT_h) - Th)/(Rih*Ch)"
        },
        "alg": {
            "Tset_sup": "Tset_sup - (T_set_sup_on)*m_flow_bool - (T_set_sup_off)*(1 - m_flow_bool)"
        },
        "w": {
            "v2": "Tret - y2",
            "v3": "Tsup - y3"
        },
        "p": [
            "T_set_sup_on",
            "T_set_sup_off",
            "Rsup",
            "Csup",
            "Rih",
            "Rrh",
            "Rsh",
            "Rir",
            "Ch",
            "Rsup_hold",
            "Csup_hold",
            "Rret",
            "Cret",
            "deltaT_h",
            "deltaT_sup",
            "deltaT_ret",
            "tau",
            "m_flow_nom",
            "cp_water"
        ],
        "u": [
            "Ta",
            "Ti",
            "m_flow_bool",
            "flow_weight",
            "Prad",
            "m_flow"
        ],
        "z": [
            "Tset_sup"
        ],
        "x": [
          "Tret",
          "Tsup",
          "Th"
        ],
        "y": [
            ["y2", "Tret"],
            ["y3", "Tsup"]
        ]
    },
    "bounds": {
        "z": {
            "lbz": [250],
            "ubz": [350]
        },
        "x": {
            "lbx": [250,250,250],
            "ubx": [350,350,350]
        }
    },
    "objective": "dot(flow_weight*v2, flow_weight*v2)@R[0,0] + dot(flow_weight*v3, flow_weight*v3)@R[1,1]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "legendre",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "",
    "opt": {
        "verbose": true,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 10,
        "ipopt.ma57_automatic_scaling" : "yes",
        "ipopt.hessian_approximation": "exact"
    },
    "codegen": false
}
