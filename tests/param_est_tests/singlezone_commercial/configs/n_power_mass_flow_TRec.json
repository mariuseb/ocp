{
    "model": {
        "alg": {
            "Pvent": "Pvent - ahu_reaFloSupAir*cp_air*(Tsup_air-ahu_reaTHeaRec)*eta_hex1",
            "ahu_reaTHeaRec": "(Ta + (ahu_reaTRetAir - Ta)*eta_hex2)*ahu_pump_sup_on + (1-ahu_pump_sup_on)*Trec_off - ahu_reaTHeaRec",
            "ahu_reaFloSupAir": "ahu_reaFloSupAir - m_flow_air_nom*regNonZeroPower(ahu_pump_sup, n, 0.01)",
            "ahu_pump_sup_on": "ahu_pump_sup_on - ca.if_else(ahu_pump_sup > 1E-4, 1, 0)"

        },
        "w": {
            "v2": "y2 - Pvent",
            "v3": "m_flow_weight*(y3 - ahu_reaTHeaRec)",
            "v4": "y4 - ahu_reaFloSupAir"
        },
        "p": [
            "m_flow_air_nom",
            "n",
            "cp_air",
            "eta_hex1",
            "eta_hex2",
            "Trec_off"

        ],
        "u": [
            "ahu_pump_sup",
            "ahu_pump_ret",
            "oveTSupSet",
            "m_flow_weight",
            "Tsup_air",
            "ahu_reaTRetAir"
        ],
        "r": [
            "Ta",
            "phi_s",
            "int_con",
            "int_lat",
            "int_rad"
        ],
        "z": [
            "Pvent",
            "ahu_reaTHeaRec",
            "ahu_reaFloSupAir",
            "ahu_pump_sup_on"
        ],
        "y": [
            ["y2", "Pvent"],
            ["y3", "ahu_reaTHeaRec"],
            ["y4", "ahu_reaFloSupAir"]
        ]
    },
    "objective": "dot(v2, v2)@R[0,0] + dot(v3, v3)@R[1,1] + dot(v4, v4)@R[2,2]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "radau",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "none",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 10,
        "ipopt.ma57_automatic_scaling" : "yes"
    },
    "codegen": false
}
