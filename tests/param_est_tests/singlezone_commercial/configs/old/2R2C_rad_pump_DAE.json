{
    "model": {
        "ode": {
            "Ti": "(Te - Ti)/(Rie*Ci) + (1/Ci)*u_pump*UA_nom*regNonZeroPower((Tsup - Ti), n, 0.05)  + Ai*(phi_s/Ci)",
            "Te": "(Ti - Te)/(Rie*Ce) + (Ta - Te)/(Rea*Ce)"
        },
        "w": {
            "v1": "Ti - y1", 
            "v2": "u_pump*UA_nom*regNonZeroPower((Tsup - Ti), n, 0.05) - y2*eta"
        },
        "p": [
            "Rie",
            "Rea",
            "Ci",
            "Ce",
            "Ai",
            "n",
            "UA_nom",
            "eta"
        ],
        "u": [
            "Tsup",
            "Ph",
            "u_pump"
        ],
        "r": [
            "Ta",
            "phi_s"
        ],
        "x": [
            "Ti",
            "Te"
        ],
        "y": [
            ["y1", "Ti"],
            ["y2", "Ph"]
        ]
    },
    "objective": "dot(v1, v1)@R[0,0]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "RK4",
        "method": "radau",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "gauss-newton",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 10,
        "ipopt.ma57_automatic_scaling" : "yes"
    },
    "codegen": true
}