{
    "model": {
        "ode": {
            "Ti": "(Te - Ti)/(Rie*Ci) + (Ta - Ti)/(Ria*Ci) + (1/Ci)*dh_pump*UA_nom*regNonZeroPower((Tsup - Ti), n, delta) + Ai*(phi_s/Ci) + rho_int*((int_con+int_lat+int_rad)/Ci) + (1/Ci)*ahu_pump_sup*UA_nom_air*(ahu_reaTSupAir - Ti)",
            "Te": "(Ti - Te)/(Rie*Ce) + (Ta - Te)/(Rea*Ce)"
        },
        "w": {
            "v1": "Ti - y1", 
            "v2": "dh_pump*UA_nom*regNonZeroPower((Tsup - Ti), n, delta) - y2*eta_rad",
            "v3": "ahu_pump_sup*UA_nom_air*(ahu_reaTSupAir - Ti) - y3*eta_hex"
        },
        "p": [
            "Rie",
            "Rea",
            "Ria",
            "Tsup",
            "rho_int",
            "UA_nom_air",
            "eta_hex",
            "Ci",
            "Ce",
            "Ai",
            "n",
            "UA_nom",
            "eta_rad",
            "delta"
        ],
        "u": [
            "dh_pump",
            "Ph",
            "Pvent",
            "ahu_pump_sup"
        ],
        "r": [
            "Ta",
            "phi_s",
            "int_con",
            "int_lat",
            "int_rad",
            "ahu_reaTSupAir"
        ],
        "x": [
            "Ti",
            "Te"
        ],
        "y": [
            ["y1", "Ti"],
            ["y2", "Ph"],
            ["y3", "Pvent"]
        ]
    },
    "objective": "dot(v1, v1)@R[0,0]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "RK4",
        "method": "radau",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "none",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 10,
        "ipopt.ma57_automatic_scaling" : "yes"
    },
    "codegen": true
}
