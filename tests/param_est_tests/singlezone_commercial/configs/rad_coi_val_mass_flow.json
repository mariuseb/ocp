{
    "model": {
        "alg": {
            "Prad": "Prad - rad_flo*cp_water*(Tsup - Tret)*eta_hex3",
            "rad_flo": "rad_flo_max*rad_flo - NN_rad(rad_val, coi_val)",
            "coi_flo": "coi_flo_max*coi_flo - NN_coi(rad_val, coi_val)"
        },
        "w": {
            "v1": "y1 - Prad",
            "v6": "y6 - rad_flo",
            "v7": "y7 - coi_flo"
        },
        "p": [
            "cp_water",
            "eta_hex3",
            "rad_flo_max",
            "coi_flo_max"
        ],
        "u": [
            "Tsup",
            "Tret",
            "rad_val",
            "coi_val"
        ],
        "r": [
            "Ta",
            "phi_s",
            "int_con",
            "int_lat",
            "int_rad"
        ],
        "z": [
            "Prad",
            "rad_flo",
            "coi_flo"
        ],
        "y": [
            ["y1", "Prad"],
            ["y6", "rad_flo"],
            ["y7", "coi_flo"]
            
        ]
    },
    "objective": "dot(v1, v1)@R[0,0] + dot(v6, v6)@R[1,1] + dot(v7, v7)@R[2,2]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "radau",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "none",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma57",
        "ipopt.ma57_pre_alloc": 10,
        "ipopt.ma57_automatic_scaling" : "yes"
    },
    "codegen": false
}
