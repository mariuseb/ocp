{
    "model": {
        "ode": {
            "Ti_sou": "(Ta - Ti_sou)/(Rsou*Csou) + phi_h_sou*(1/Csou)",
            "Ti_nor": "(Ta - Ti_nor)/(Rnor*Cnor) + phi_h_nor*(1/Cnor)"
        },
        "w": {
            "v1": "Ti_sou - y1",
            "v2": "Ti_nor - y2"
        },
        "p": [
            "Rsou",
            "Csou",
            "Rnor",
            "Cnor"
        ],
        "u": [
            "phi_h_sou",
            "phi_h_nor"
        ],
        "r": [
            "Ta"
        ],
        "x": [
            "Ti_sou",
            "Ti_nor"
        ],
        "y": [
            ["y1", "Ti_sou"],
            ["y2", "Ti_nor"]
        ]
    },
    "objective": "dot(v1, v1)@R[0,0] + dot(v2, v2)@R[1,1] + dot(s1, s1)@Q [0,0] + dot(s2, s2)@Q[1,1]",
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "RK4",
        "method": "legendre",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "mumps",
        "ipopt.ma57_pre_alloc": 10,
        "ipopt.ma57_automatic_scaling" : "yes"
    },
    "codegen": true
}
